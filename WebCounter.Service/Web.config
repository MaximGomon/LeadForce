            </service>-->
        <service name="WebCounter.Service.CounterService">
          <endpoint address=""
                  binding="customBinding"
                  bindingConfiguration="jsonpBinding"
                  behaviorConfiguration="CounterServiceBehavior"
                  contract="WebCounter.Service.ICounterService">
          </endpoint>
        </service>
      </services>
    </system.serviceModel>      
